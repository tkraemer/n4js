<?eclipse version="3.4"?>
<plugin>
   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="eu.numberfour.n4js.product.N4JSApplication">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="eu.numberfour.n4js.product.N4JSPerspective"
            icon="icons/n4_logo_16.png"
            id="eu.numberfour.n4js.product.N4JSPerspective"
            name="N4JS Perspective">
      </perspective>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="eu.numberfour.n4js.product.application"
            name="N4JS IDE">
         <property
               name="appName"
               value="N4JS IDE">
         </property>
         <property	
               name="windowImages"
               value="icons/n4_logo_16.png,icons/n4_logo_32.png,icons/n4_logo_48.png,icons/n4_logo_64.png,icons/n4_logo_128.png,icons/n4_logo_256.png">
         </property>
         <property
               name="aboutImage"
               value="icons/n4_logo_128.png">
         </property>
         <property
               name="aboutText"
               value="%aboutText">
         </property>
         <property
               name="includesBuiltInLibraries"
               value="true">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="eu.numberfour.n4js.product.N4JSPerspective">
         
         <newWizardShortcut id="eu.numberfour.n4js.ui.wizard.N4JSNewClassWizard"/>
         <newWizardShortcut id="eu.numberfour.n4js.ui.wizard.N4JSNewInterfaceWizard"/>
         <newWizardShortcut id="eu.numberfour.n4js.n4mf.ui.wizard.N4MFNewProjectWizard"/>
         
         <view
               id="org.eclipse.ui.navigator.ProjectExplorer"
               minimized="false"
               ratio="0.25"
               relationship="left"
               relative="org.eclipse.ui.editorss">
         </view>
         <view
               id="org.eclipse.ui.views.ProblemView"
               minimized="false"
               ratio="0.75"
               relationship="bottom"
               relative="org.eclipse.ui.editorss">
         </view>
         <view
               id="org.eclipse.ui.console.ConsoleView"
               minimized="false"
               relationship="stack"
               relative="org.eclipse.ui.views.ProblemView">
         </view>
         <view
               id="org.eclipse.team.ui.GenericHistoryView"
               minimized="false"
               relationship="stack"
               relative="org.eclipse.ui.console.ConsoleView">
         </view>
         <view
               id="eu.numberfour.n4js.tester.ui.TestResultsView"
               minimized="false"
               relationship="stack"
               relative="org.eclipse.team.ui.GenericHistoryView">
         </view>
         <view
               id="org.eclipse.ui.views.ContentOutline"
               minimized="false"
               ratio="0.75"
               relationship="right"
               relative="org.eclipse.ui.editorss">
         </view>
         <actionSet
               id="org.eclipse.debug.ui.launchActionSet">
         </actionSet>
      </perspectiveExtension>
   </extension>
