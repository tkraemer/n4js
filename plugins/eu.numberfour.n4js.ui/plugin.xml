<plugin>
   <extension-point id="composedgenerator" name="N4JS Composed Generator" schema="schema/composedgenerator.exsd"/>
   <extension-point id="jsengine" name="N4 JavaScript Engine" schema="schema/jsengine.exsd"/>
   <extension-point id="resourceDescriptionsPersisterContribution" name="Resource Descriptions Persister Contribution" schema="schema/resourceDescriptionsPersisterContribution.exsd"/>

<!-- CUSTOM CODE -->
<!--
  A specialized Types representation is registered for n4ts files that are loaded
  with the built-in scheme, e.g. 'n4scheme:/global.n4ts'
-->

	<extension
         point="org.eclipse.ui.newWizards">
          <category     
        id="eu.numberfour.wizards" 
        name="NumberFour">
      </category>
     <wizard
           category="eu.numberfour.wizards"
           class="eu.numberfour.n4js.ui.N4JSExecutableExtensionFactory:eu.numberfour.n4js.ui.wizard.classes.N4JSNewClassWizard"
           icon="icons/newclass_wiz.gif"
           id="eu.numberfour.n4js.ui.wizard.N4JSNewClassWizard"
           name="N4JS Class"
           project="false">
           <description>
           	Create a new N4JS Class file or create a new class inside an existing module.
           </description>
      </wizard>
      <wizard
           category="eu.numberfour.wizards"
           class="eu.numberfour.n4js.ui.N4JSExecutableExtensionFactory:eu.numberfour.n4js.ui.wizard.interfaces.N4JSNewInterfaceWizard"
           icon="icons/newint_wiz.gif"
           id="eu.numberfour.n4js.ui.wizard.N4JSNewInterfaceWizard"
           name="N4JS Interface"
           project="false">
           <description>
           	Create a new N4JS Interface file or create a new interface inside an existing module.
           </description>
      </wizard>
      
	</extension>

