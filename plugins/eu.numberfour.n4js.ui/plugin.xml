            name="N4JS">
            <keywordReference id="eu.numberfour.n4js.ui.keyword_N4JS"/>
            <enabledWhen>
	            <adapt type="org.eclipse.core.resources.IProject"/>
			</enabledWhen>
	        <filter name="projectNature" value="org.eclipse.xtext.ui.shared.xtextNature"/>
        </page>
    </extension>
    <extension
        point="org.eclipse.ui.keywords">
        <keyword
            id="eu.numberfour.n4js.ui.keyword_N4JS"
