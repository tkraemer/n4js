      <command
            id="eu.numberfour.n4js.ui.search.openTypeSelectionDialog"
            name="Open N4JS Type">
      </command>
      <command
            id="eu.numberfour.n4js.ui.showTypeInformation"
            name="Show Type Information">
      </command>
      <command
            id="eu.numberfour.n4js.ui.openGeneratedSourceInEditor"
            name="Open Generated Source In Editor">
      </command>
