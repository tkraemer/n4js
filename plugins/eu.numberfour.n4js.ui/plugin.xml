    <extension point="org.eclipse.ui.menus">
        <menuContribution
            locationURI="popup:#TextEditorContext?after=group.edit">
             <command
                 commandId="eu.numberfour.n4js.N4JS.validate"
                 style="push"
                 tooltip="Trigger expensive validation">
            <visibleWhen checkEnabled="false">
                <reference
                    definitionId="eu.numberfour.n4js.N4JS.Editor.opened">
                </reference>
            </visibleWhen>
         </command>  
         </menuContribution>
         <!-- copy qualified name -->
         <menuContribution locationURI="popup:#TextEditorContext?after=copy">
         	<command commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName" 
         		style="push" tooltip="Copy Qualified Name">
            	<visibleWhen checkEnabled="false">
                	<reference definitionId="eu.numberfour.n4js.N4JS.Editor.opened" />
            	</visibleWhen>
         	</command>  
         </menuContribution>
         <menuContribution locationURI="menu:edit?after=copy">
         	<command commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName"
            	style="push" tooltip="Copy Qualified Name">
            	<visibleWhen checkEnabled="false">
                	<reference definitionId="eu.numberfour.n4js.N4JS.Editor.opened" />
            	</visibleWhen>
         	</command>  
         </menuContribution>
         <menuContribution locationURI="popup:org.eclipse.xtext.ui.outline?after=additions">
			<command commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName" 
				style="push" tooltip="Copy Qualified Name">
         		<visibleWhen checkEnabled="false">
	            	<and>
	            		<reference definitionId="eu.numberfour.n4js.N4JS.XtextEditor.opened" />
						<iterate>
							<adapt type="org.eclipse.xtext.ui.editor.outline.IOutlineNode" />
						</iterate>
					</and>
				</visibleWhen>
			</command>
         </menuContribution>
         <menuContribution
               allPopups="false"
               locationURI="toolbar:org.eclipse.ui.main.toolbar">
            <toolbar
                  id="eu.numberfour.n4js.ui.toolbar">
               <command
                     commandId="eu.numberfour.n4js.ui.search.openTypeSelectionDialog"
                     icon="icons/opentype.gif"
                     label="Open N4JS Type"
                     style="push">
               </command>
               <command
                     commandId="eu.numberfour.n4js.ui.handler.OpenExternalLibraryPreferences"
                     icon="icons/native_lib_path_attrib.gif"
                     label="Open Built-in Libraries Preferences"
                     style="push">
               </command>
            </toolbar>
         </menuContribution>
         <menuContribution
               allPopups="false"
               locationURI="popup:org.eclipse.ui.navigator.ProjectExplorer#PopupMenu?after=additions">
            <command
                  commandId="eu.numberfour.n4js.ui.openGeneratedSourceInEditor"
                  icon="icons/macrodef_obj.gif"
                  label="Open Generated Source In Editor"
                  style="push">
               <visibleWhen
                     checkEnabled="false">
                  <test
                        property="eu.numberfour.n4js.ui.fileExtension"
                        value="n4js, js">
                  </test>
               </visibleWhen>
            </command>
         </menuContribution>
    </extension>
